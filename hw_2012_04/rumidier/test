			if ('N'=='Y') {
				url=url+\"&isOnlyViewEE=N\";
			} else {
				url=url+\"&isOnlyViewEE=Y\";
			}			
			window.location.href=url;
	 	}	  
	}
	
	function searchEE2EKChange(obj){
		var searchOptionStr=\"\";
		
		if(searchOptionStr==\"example\"){
			jindo.\$Cookie().set(\"small_deleteExampleCookie\", \"false\");
			location.href = \"popSearchExample.nhn?query=%EB%9C%A8%EA%B2%81%EB%8B%A4&isOnlyViewEE=\"+obj.value;
		}else{
			location.href = \"/popManager.nhn?m=search&query=\"+'%EB%9C%A8%EA%B2%81%EB%8B%A4'+\"&searchOption=\"+searchOptionStr+\"&isOnlyViewEE=\"+obj.value;
		}
	}
	
	function searchChange(obj){	
		location.href = \"/popManager.nhn?m=search&query=\"+'%EB%9C%A8%EA%B2%81%EB%8B%A4'+\"&searchOption=\"+obj.value;
	}	
	
